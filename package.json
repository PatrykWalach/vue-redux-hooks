{
  "name": "vue-redux-hooks",
  "type": "module",
  "version": "1.2.0",
  "description": "Redux hooks for Vue",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": {
        "module-sync": "./dist/index.js",
        "default": "./dist/index.cjs"
      },
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./query": {
      "types": "./dist/query/index.d.ts",
      "node": {
        "module-sync": "./dist/query/index.js",
        "default": "./dist/query/index.cjs"
      },
      "import": "./dist/query/index.js",
      "default": "./dist/query/index.cjs"
    }
  },
  "files": ["dist", "src"],
  "scripts": {
    "pkg-pr-new-publish": "pkg-pr-new publish",
    "build": "tsup src/index.ts src/query/index.js --format cjs --dts --clean",
    "prepublishOnly": "pnpm run build",
    "dev": "tsc --watch",
    "codecov": "codecov",
    "lint": "biome check",
    "test": "cypress run --component",
    "cypress": "cypress open --component"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PatrykWalach/vue-redux-hooks.git"
  },
  "keywords": [
    "vue",
    "composition-api",
    "redux",
    "hooks",
    "typescript",
    "rtk-query",
    "query"
  ],
  "author": "Patryk WaÅ‚ach",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PatrykWalach/vue-redux-hooks/issues"
  },
  "homepage": "https://github.com/PatrykWalach/vue-redux-hooks#readme",
  "sideEffects": false,
  "peerDependencies": {
    "@reduxjs/toolkit": ">=1.6.0",
    "@vue/composition-api": "^1.0.0-rc.1",
    "redux": ">=4.0.0",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cypress/code-coverage": "^3.13.7",
    "@reduxjs/toolkit": "1.8.2",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^4.1.0",
    "cypress": "^13.16.0",
    "husky": "^4.3.8",
    "lint-staged": "13.0.0",
    "redux": "^4.2.0",
    "tsup": "^6.0.1",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-plugin-istanbul": "^6.0.2",
    "vue": "^3.2.36"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": "eslint --cache --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "pkg-pr-new": "^0.0.30",
    "vue-demi": "^0.14.10"
  },
  "packageManager": "pnpm@9.14.2"
}
