{
  "name": "vue-redux-hooks",
  "type": "commonjs",
  "version": "1.2.0",
  "description": "Redux hooks for Vue",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": {
        "module-sync": "./dist/index.mjs",
        "default": "./dist/index.js"
      },
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./query": {
      "types": "./dist/query/index.d.ts",
      "node": {
        "module-sync": "./dist/query/index.mjs",
        "default": "./dist/query/index.js"
      },
      "import": "./dist/query/index.mjs",
      "default": "./dist/query/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/query/index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "bun run build",
    "dev": "bun run build -- --watch src",
    "codecov": "codecov",
    "lint": "eslint --ext .js,.jsx,.vue,.ts,.tsx --fix src",
    "test": "cypress run --component",
    "cypress": "cypress open --component"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PatrykWalach/vue-redux-hooks.git"
  },
  "keywords": [
    "vue",
    "composition-api",
    "redux",
    "hooks",
    "typescript",
    "rtk-query",
    "query"
  ],
  "author": "Patryk WaÅ‚ach",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PatrykWalach/vue-redux-hooks/issues"
  },
  "homepage": "https://github.com/PatrykWalach/vue-redux-hooks#readme",
  "sideEffects": false,
  "peerDependencies": {
    "@reduxjs/toolkit": ">=1.6.0",
    "@vue/composition-api": "^1.0.0-rc.1",
    "redux": ">=4.0.0",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@reduxjs/toolkit": "1.8.2",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "cypress": "^10.0.1",
    "eslint": "^8.16.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.0",
    "husky": "^4.3.8",
    "lint-staged": "13.0.0",
    "prettier": "^2.6.2",
    "redux": "^4.2.0",
    "tsup": "^6.0.1",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vite-plugin-istanbul": "^2.7.3",
    "vue": "^3.2.36"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": "eslint --cache --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "vue-demi": "^0.13.1"
  },
  "packageManager": "bun@1.1.29"
}
