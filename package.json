    "@typescript-eslint/parser": "^2.23.0",
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "redux": "^4.0.5",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3",
    "vue": "^3.0.0-alpha.8"
