{
  "name": "vue-redux-hooks",
  "version": "0.1.6",
  "description": "Redux hooks for Vue",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  },
  "scripts": {
    "build": "pika build",
    "coverage": "jest --collect-coverage",
    "codecov": "codecov",
    "lint": "eslint --fix src/**/*.ts{,x}",
    "test": "jest",
    "watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PatrykWalach/vue-redux-hooks.git"
  },
  "keywords": [
    "vue",
    "composition-api",
    "redux",
    "hooks",
    "typescript"
  ],
  "author": "Patryk WaÅ‚ach",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PatrykWalach/vue-redux-hooks/issues"
  },
  "homepage": "https://github.com/PatrykWalach/vue-redux-hooks#readme",
  "sideEffects": false,
  "peerDependencies": {
    "@vue/composition-api": ">=0.3.2",
    "redux": ">=4.0.4",
    "vue": ">=2.6.10"
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-types": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-standard-pkg": "^0.9.2",
    "@reduxjs/toolkit": "^1.3.6",
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.10",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@vue/composition-api": "^0.5.0",
    "@vue/test-utils": "^1.0.3",
    "codecov": "^3.7.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.5.4",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5",
    "redux": "^4.0.5",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.5",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
