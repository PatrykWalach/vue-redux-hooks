    "cypress": "^12.17.3",
    "eslint": "^8.46.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.16.1",
    "husky": "^8.0.3",
    "lint-staged": "13.2.3",
    "prettier": "^3.0.1",
    "redux": "^4.2.1",
    "typescript": "^5.1.6",
